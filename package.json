{
  "name": "@tubular/math",
  "version": "3.0.0-alpha.0",
  "description": "Miscellaneous math functions",
  "browser": "dist/umd/index.js",
  "cjs": "dist/cjs/index.cjs",
  "main": "dist/fesm2015/index.js",
  "module": "dist/fesm2015/index.js",
  "esm2015": "dist/index.js",
  "fesm2015": "dist/fesm2015/index.js",
  "typings": "dist/index",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist/ && tsc && webpack && webpack --env umd=true && webpack --env cjs=true",
    "prepublishOnly": "npm run build",
    "lint": "eslint 'src/**/*.ts'",
    "test": "nyc --reporter=html mocha --require ts-node/register src/**/*.spec.ts"
  },
  "keywords": [
    "angle",
    "math",
    "min-max",
    "spherical",
    "zero"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": "github:kshetline/tubular_math"
}
